    "readable-stream": "^4.7.0",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "util": "^0.12.5",
    "vite": "^6.2.0",
